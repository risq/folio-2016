{
  "name": "folio-2016",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "build": "npm run thumbnails && webpack --config webpack-prod.config.js --colors",
    "dev": "webpack-dev-server --config webpack-dev.config.js --watch --colors",
    "thumbnails": "mogrify -path src/static/images/projects/small -thumbnail 480x270 src/static/images/projects/*.jpg",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/risq/folio-2016.git"
  },
  "keywords": [
    "webpack",
    "startkit",
    "frontend",
    "es6",
    "javascript",
    "webdev"
  ],
  "author": "webkid.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/risq/folio-2016/issues"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "semi": 2
    }
  },
  "homepage": "https://github.com/risq/folio-2016#readme",
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.10.2",
    "eslint-loader": "^1.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^2.17.0",
    "node-sass": "^3.7.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.0",
    "bluebird": "^3.4.1",
    "debounce": "^1.0.0",
    "eventemitter3": "^2.0.2",
    "jquery": "^3.1.0",
    "sass-mq": "^3.2.9",
    "simplezoom": "git+https://github.com/risq/simplezoom.git",
    "transceiver": "^0.3.4",
    "velocity-animate": "^1.3.1"
  }
